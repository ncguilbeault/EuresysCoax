<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Euresys Coax Library</Title>
    <Description>Library containing modules for interfacing Euresys Coaxlink boards with Bonsai.</Description>
    <Authors>Nicholas Guilbeault</Authors>
    <Copyright>Copyright © Nicholas Guilbeault 2022</Copyright>
    <PackageOutputPath>..\bin\$(Configuration)</PackageOutputPath>
    <PackageTags>Bonsai Rx Euresys Coax</PackageTags>
    <IncludeSymbols Condition="'$(Configuration)'=='Release'">true</IncludeSymbols>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <TargetFramework>net472</TargetFramework>
    <Features>strict</Features>
    <Version>1.0.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.6.2" />
    <PackageReference Include="OpenCV.Net" Version="3.3.1" />
    <PackageReference Include="Coaxlink_NetApi" Version="1.0.0" />
  </ItemGroup>

</Project>